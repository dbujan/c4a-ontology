[{"@id":"http://morelab.deusto.es/ontologies/riskC4A#","@type":["http://www.w3.org/2002/07/owl#Ontology"]},{"@id":"http://morelab.deusto.es/ontologies/riskC4A#Category","@type":["http://www.w3.org/2002/07/owl#Class"],"http://www.w3.org/2000/01/rdf-schema#comment":[{"@value":"Group of Items (see below) that contribute to the appraisal of a certain facet of the measured health entity."}]},{"@id":"http://morelab.deusto.es/ontologies/riskC4A#Instrument","@type":["http://www.w3.org/2002/07/owl#Class"],"http://www.w3.org/2000/01/rdf-schema#comment":[{"@value":"Overall measuring method, aimed at providing a final, comprehensive score on a certain health entity."}]},{"@id":"http://morelab.deusto.es/ontologies/riskC4A#Interval","@type":["http://www.w3.org/2002/07/owl#Class"],"http://www.w3.org/2000/01/rdf-schema#comment":[{"@value":"Difference among numbers is meaningful like the concept of unit of measure (e.g. temperature in ºC)."}],"http://www.w3.org/2000/01/rdf-schema#subClassOf":[{"@id":"http://morelab.deusto.es/ontologies/riskC4A#Score"}]},{"@id":"http://morelab.deusto.es/ontologies/riskC4A#Item","@type":["http://www.w3.org/2002/07/owl#Class"],"http://www.w3.org/2000/01/rdf-schema#comment":[{"@value":"Each actual, basic element that will be measured to produce scores."}]},{"@id":"http://morelab.deusto.es/ontologies/riskC4A#Model","@type":["http://www.w3.org/2002/07/owl#Class"],"http://www.w3.org/2000/01/rdf-schema#comment":[{"@value":"Mathematical entity (algorithms and/or calculations) that allows to compute the Instrument outcome score on the basis of the scores measured for single Items."}]},{"@id":"http://morelab.deusto.es/ontologies/riskC4A#Nominal","@type":["http://www.w3.org/2002/07/owl#Class"],"http://www.w3.org/2000/01/rdf-schema#comment":[{"@value":"Numbers are used just as labels for categories (e.g. 0 for false and 1 for true, for a classifier outcome)."}],"http://www.w3.org/2000/01/rdf-schema#subClassOf":[{"@id":"http://morelab.deusto.es/ontologies/riskC4A#Score"}]},{"@id":"http://morelab.deusto.es/ontologies/riskC4A#Ordinal","@type":["http://www.w3.org/2002/07/owl#Class"],"http://www.w3.org/2000/01/rdf-schema#comment":[{"@value":"Order is meaningful and represents growing quantities (e.g. 1 = mild, 2 = moderate, 3 = strong)."}],"http://www.w3.org/2000/01/rdf-schema#subClassOf":[{"@id":"http://morelab.deusto.es/ontologies/riskC4A#Score"}]},{"@id":"http://morelab.deusto.es/ontologies/riskC4A#Ratio","@type":["http://www.w3.org/2002/07/owl#Class"],"http://www.w3.org/2000/01/rdf-schema#comment":[{"@value":"There is a meaningful zero point, so ratios among scores are also meaningful (e.g. number of seconds to complete an action)."}],"http://www.w3.org/2000/01/rdf-schema#subClassOf":[{"@id":"http://morelab.deusto.es/ontologies/riskC4A#Score"}]},{"@id":"http://morelab.deusto.es/ontologies/riskC4A#Score","@type":["http://www.w3.org/2002/07/owl#Class"],"http://www.w3.org/2000/01/rdf-schema#comment":[{"@value":"The outcome of measuring an Item (or the outcome of the overall Instrument)."}]},{"@id":"http://morelab.deusto.es/ontologies/riskC4A#Value","@type":["http://www.w3.org/2002/07/owl#Class"],"http://www.w3.org/2000/01/rdf-schema#comment":[{"@value":"Set of values associated to an Item. The Item’s measurement produces a specific score, drawn from this set."}]},{"@id":"http://morelab.deusto.es/ontologies/riskC4A#hasCategories","@type":["http://www.w3.org/2002/07/owl#ObjectProperty"],"http://www.w3.org/2000/01/rdf-schema#domain":[{"@id":"http://morelab.deusto.es/ontologies/riskC4A#Instrument"}],"http://www.w3.org/2000/01/rdf-schema#range":[{"@id":"http://morelab.deusto.es/ontologies/riskC4A#Category"}]},{"@id":"http://morelab.deusto.es/ontologies/riskC4A#hasItems","@type":["http://www.w3.org/2002/07/owl#ObjectProperty"],"http://www.w3.org/2000/01/rdf-schema#domain":[{"@id":"http://morelab.deusto.es/ontologies/riskC4A#Category"}],"http://www.w3.org/2000/01/rdf-schema#range":[{"@id":"http://morelab.deusto.es/ontologies/riskC4A#Item"}]},{"@id":"http://morelab.deusto.es/ontologies/riskC4A#hasModel","@type":["http://www.w3.org/2002/07/owl#ObjectProperty"],"http://www.w3.org/2000/01/rdf-schema#domain":[{"@id":"http://morelab.deusto.es/ontologies/riskC4A#Instrument"}],"http://www.w3.org/2000/01/rdf-schema#range":[{"@id":"http://morelab.deusto.es/ontologies/riskC4A#Model"}]},{"@id":"http://morelab.deusto.es/ontologies/riskC4A#hasScore","@type":["http://www.w3.org/2002/07/owl#ObjectProperty"],"http://www.w3.org/2000/01/rdf-schema#domain":[{"@id":"http://morelab.deusto.es/ontologies/riskC4A#Instrument"},{"@id":"http://morelab.deusto.es/ontologies/riskC4A#Item"}],"http://www.w3.org/2000/01/rdf-schema#range":[{"@id":"http://morelab.deusto.es/ontologies/riskC4A#Score"}]},{"@id":"http://morelab.deusto.es/ontologies/riskC4A#hasValue","@type":["http://www.w3.org/2002/07/owl#ObjectProperty"],"http://www.w3.org/2000/01/rdf-schema#domain":[{"@id":"http://morelab.deusto.es/ontologies/riskC4A#Item"}],"http://www.w3.org/2000/01/rdf-schema#range":[{"@id":"http://morelab.deusto.es/ontologies/riskC4A#Value"}]},{"@id":"http://morelab.deusto.es/ontologies/riskC4A#hasValues","@type":["http://www.w3.org/2002/07/owl#ObjectProperty"],"http://www.w3.org/2000/01/rdf-schema#domain":[{"@id":"http://morelab.deusto.es/ontologies/riskC4A#Model"}],"http://www.w3.org/2000/01/rdf-schema#range":[{"@id":"http://morelab.deusto.es/ontologies/riskC4A#Value"}]}]